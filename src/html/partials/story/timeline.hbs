<div class='timeline-box'>
    <div class='scroll-hint is-visible'>
      <p>Scroll</p>
      <img inline src='arrow-down.svg'>
    </div>
    <div class='timeline-svg-box'>
        <div class='arrow-up-svg'>
          <p>Return to Start</p>
                <div class='button-circle'>
                <img inline src='arrow-up.svg'>
            </div>
        </div>
        <div class='tooltip hidden'></div>
        <svg class='timeline-svg'></svg>
    </div>
</div>

<div class='conclusion'>
                {{#each doc.conclusion}}
                <p>{{{value}}}</p>
            {{/each}}
</div>
<div class='method'>
            {{#each doc.Method}}
                <p>{{{value}}}</p>
            {{/each}}
</div>

